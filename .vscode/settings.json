// Place your settings in this file to overwrite default and user settings.
{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.rulers": [80],
  "tslint.enable": true,
  "editor.fontSize": 12,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "build": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/build": true
  },
  "tslint.autoFixOnSave": true,
  "editor.minimap.enabled": true,
  "cSpell.words": [
